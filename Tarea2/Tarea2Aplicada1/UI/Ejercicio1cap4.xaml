<Window x:Class="Tarea2Aplicada1.UI.Ejercicio1cap4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tarea2Aplicada1.UI"
        mc:Ignorable="d"
        Title="Ejercicio1cap4" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="10">
            <Label Content="Ingrese el numero del cual desea la tabla:"/>
            <StackPanel Orientation="Horizontal">
                <TextBox x:Name="NumeroTextBox" Height="20" Width="50" Margin="10" />
                <Button x:Name="ConfirmarButton" Content="Confirmar" Height="20" Width="100" Margin="60,0" Click="ConfirmarButton_Click"/>
            </StackPanel>

            <StackPanel x:Name="PanelGeneral" Orientation="Vertical">
                
            </StackPanel>
            
        </StackPanel>
    </Grid>
</Window>
